	"printWidth": 180,
	"semi": false,
	"tabWidth": 2,
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "all",
	"bracketSpacing": true,
	"htmlWhitespaceSensitivity": "ignore",
	"arrowParens": "avoid",
	"parsers": {
		".jsx": "flow",
		".scss": "scss",
		".ts": "typescript",
		".less": "css",
		".vue": "vue",
		".nvue": "vue",
		".ux": "vue",
		".yml": "yaml"
	}
}
